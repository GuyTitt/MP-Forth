from memoire import mem

class Piles:
    async def push(self, x): 
        mem.sp -= 4; mem.wpoke(mem.sp, x)
    async def pop(self):
        if mem.sp >= mem.SP0: raise IndexError("underflow")
        v = mem.wpeek(mem.sp); mem.sp += 4; return v

piles = Piles()
version = ('piles.py', 6)
